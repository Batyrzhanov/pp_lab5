def find_chickens_and_rabbits(num_heads, num_legs):
    for num_chickens in range(num_heads + 1):
        num_rabbits = num_heads - num_chickens
        if (2 * num_chickens + 4 * num_rabbits) == num_legs:
            return num_chickens, num_rabbits
    return None, None

num_heads = int(input("Enter the number of heads: "))
num_legs = int(input("Enter the number of legs: "))

chickens, rabbits = find_chickens_and_rabbits(num_heads, num_legs)

if chickens is not None:
    print("Number of chickens:", chickens)
    print("Number of rabbits:", rabbits)
else:
    print("No solution found.")

