def is_spy_game_sequence(numbers):
    zero_count = 0 

    for num in numbers:
        if num == 0:
            zero_count += 1
        elif zero_count >= 2 and num == 7:
            return True

    return False

user_input = input("Enter a list of numbers separated by spaces: ")
input_numbers = list(map(int, user_input.split()))
result = is_spy_game_sequence(input_numbers)
print("Is it a spy game sequence?", result)
